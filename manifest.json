{
  "manifest_version": 2,
  "name": "ATab",
  "version": "1.1.2",
  "description": "New Tab extension for Awesome people.",
  "homepage_url": "https://github.com/a13ks3y/ATab",
  "icons": {
    "16": "icons/ATab-16x16.png",
    "32": "icons/ATab-32x32.png",
    "48": "icons/ATab-48x48.png",
    "128": "icons/ATab-128x128.png"
  },
  "options_page": "src/options_custom/index.html",
  "browser_action": {
    "default_icon": "icons/ATab-32x32.png",
    "default_title": "ðŸ˜Ž",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "web_accessible_resources": [
    "icons/ATab-16x16.png",
    "icons/ATab-32x32.png",
    "icons/ATab-48x48.png",
    "icons/ATab-128x128.png"
  ],
  "permissions": ["tabs", "storage", "unlimitedStorage"]
}
